[Desktop Entry]
Type=Application
Name=%APP_NAME%
Comment=Open source classic 68k Mac OS emulator
Icon=%ICON_NAME%
Exec=%EXEC_NAME%
Terminal=false
StartupNotify=false
Categories=Emulator;System;
Actions=new-window;launch-settings;

[Desktop Action new-window]
Name=Start %APP_NAME%
Exec=%EXEC_NAME% --nogui true

[Desktop Action launch-settings]
Name=Settings
Exec=%EXEC_NAME% --nogui false
